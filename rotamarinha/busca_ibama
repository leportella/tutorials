import urllib2
from BeautifulSoup import BeautifulSoup

url = "http://www.brasil.gov.br/meio-ambiente/ultimas-noticias-meio-ambiente"
text = urllib2.urlopen(url).read();
soup = BeautifulSoup(text);

data = soup.findAll('div',attrs={'class':'tileContent'});

links=[]
for div in data:
    Tag_a = div.findAll('a')
    for a in Tag_a:
    	temp = a['href'].split('/')
    	for i in temp:
    		if i=='meio-ambiente':
    			links.append(a['href'])



